{
    "files.defaultLanguage": "c", 
    "c-cpp-flylint.cppcheck.enable": false,
    "c-cpp-flylint.flexelint.enable": false,
    "c-cpp-flylint.run": "onType",
    "c-cpp-flylint.clang.includePaths": [
        "${workspaceFolder}/**",
        "C:\\Program Files\\LLVM\\gnu.c.lib\\include",
        "C:\\Program Files\\LLVM\\gnu.c.lib\\include\\bits",
        "C:\\Program Files\\LLVM\\gnu.c.lib\\include\\c++\\4.8.2",
        "C:\\Program Files\\LLVM\\gnu.c.lib\\include\\c++\\4.8.2\backward",
        "C:\\Program Files\\LLVM\\gnu.c.lib\\include\\c++\\4.8.2\\bits",
        "C:\\Program Files\\LLVM\\gnu.c.lib\\include\\c++\\4.8.2\\x86_64-redhat-linux",
        "C:\\Program Files\\LLVM\\gnu.c.lib\\include\\c++\\4.8.2\\ext"
    ],
    "c-cpp-flylint.clang.defines": [
        "_DEBUG", 
        "UNICODE",
        "__GNUC__=7",
        "__cdecl=__attribute__((__cdecl__))"
    ],
    "c-cpp-flylint.standard": [
        "c11",
        "c++1z"
    ],
    "code-runner.runInTerminal": true, 
    "code-runner.executorMap": {
        "c": "cd $dir && clang $fileName -o $fileNameWithoutExt.exe -Wall -g -Og -static-libgcc -fcolor-diagnostics --target=x86_64-w64-mingw -std=c11 && $dir$fileNameWithoutExt",
        "cpp": "cd $dir && clang++ $fileName -o $fileNameWithoutExt.exe -Wall -g -Og -static-libgcc -fcolor-diagnostics --target=x86_64-w64-mingw -std=c++1z -lpthread && $dir$fileNameWithoutExt"
    },
    "code-runner.saveFileBeforeRun": true,
    "C_Cpp.clang_format_sortIncludes": true,
    "C_Cpp.errorSquiggles": "Disabled",
    "files.associations": {
        "string": "cpp",
        "deque": "cpp",
        "iostream": "cpp",
        "functional": "cpp",
        "algorithm": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "*.tcc": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
